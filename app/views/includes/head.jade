head(prefix="og: http://ogp.me/ns# hireable: http://ogp.me/ns/apps/hireable#")
  meta(charset="utf-8")
  meta(name="viewport" content="width=device-width, initial-scale=1")
  meta(http-equiv="Content-Type", content="text/html; charset=utf-8")
  meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
  meta(http-equiv="Content-Language", content="en-us")
  if titlePage
    title= titlePage
  else
    title= CONFIG.APP.name
  meta(http-equiv="Content-type", content="text/html;charset=UTF-8")
  //- meta(name="keywords", content=pkg.keywords.join(","))
  meta(name="description", content=pkg.description)
  link(href=CONFIG.CDN_HOST + "/favicon.png", rel="shortcut icon", type="image/x-icon")

  meta(property="og:title", content=pkg.name + ' - ' + title)
  meta(property="og:description", content=pkg.description)
  meta(property="og:url", content="")
  meta(property="og:image", content="")
  meta(property="og:site_name", content="#{pkg.name}")

  script(src=CONFIG.CDN_HOST +'/js/libs/modernizr.custom.js')

  link( rel="stylesheet" href=CONFIG.CDN_HOST + "/css/bootstrap.css" type="text/css")
  link( rel="stylesheet" href=CONFIG.CDN_HOST + "/css/web.css" type="text/css")

  //[if lt IE 9]> <script src="js/ie/html5shiv.js"></script> <script src="js/ie/respond.min.js"></script> <script src="js/ie/excanvas.js"></script> <![endif]

if bodyClass
  body(class="apps #{bodyClass}")
else
  body.apps
